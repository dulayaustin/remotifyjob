<div id="<%= dom_id applicant %>">
  <p>
    <strong>Name</strong>
    <%= applicant.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= applicant.email_address %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= applicant.phone %>
  </p>

  <p>
    <strong>Stage:</strong>
    <%= applicant.stage %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= applicant.status %>
  </p>

  <p>
    <strong>Job:</strong>
    <%= applicant.job_id %>
  </p>

  <% if applicant.resume.attached? %>
    <div class="w-full">
      <iframe src="<%= url_for(applicant.resume) %>" width="100%" height="1000"></iframe>
    </div>
  <% end %>
</div>
